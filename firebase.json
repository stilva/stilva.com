{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "dist/*.map",
      "dist/stats-fe.json"
    ],
    "cleanUrls": true,
    "headers": {
      "source": "**/*.@(html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=60"
        }
      ]
    }
  }
}
