{
  "sourceMaps": true,
  "presets": [
    ["@babel/preset-env", {
      "targets": "last 1 year"
    }],
    "@babel/preset-react",
    "linaria/babel"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@loadable/babel-plugin"
  ]
}
