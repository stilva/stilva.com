{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "*.map",
      "stats-fe.json"
    ],
    "cleanUrls": true,
    "headers": {
      "source": "**/*.@(html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=60"
        }
      ]
    },
    "rewrites": [
      {
        "source": "/uxe/**",
        "function": "uxe"
      }
    ]
  }
}
